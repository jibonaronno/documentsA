an><span class=3D"p">,</span> <span class=3D"s1">'config.py'</span><span cl=
ass=3D"p">,</span> <span class=3D"s1">'tests.py'</span><span class=3D"p">]<=
/span>
<span class=3D"gp">&gt;&gt;&gt; </span><span class=3D"k">with</span> <span =
class=3D"n">tarfile</span><span class=3D"o">.</span><span class=3D"n">open<=
/span><span class=3D"p">(</span><span class=3D"s1">'packages.tar.gz'</span>=
<span class=3D"p">,</span> <span class=3D"n">mode</span><span class=3D"o">=
=3D</span><span class=3D"s1">'w:gz'</span><span class=3D"p">)</span> <span =
